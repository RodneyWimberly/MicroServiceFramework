{
  "ID": "${SERVICE_ID}",
  "Name": "${SERVICE_NAME}",
  "Tags": [ $SERVICE_TAGS ],
  "Address": "${ETH0_IP}",
  "Port": ${SERVICE_PORT},
  "Checks": [
    {
      "Id": "${SERVICE_ID}-ping",
      "Name": "${SERVICE_ID} Ping",
      "Args": ["ping", "-q", "-w 1", "${ETH0_IP}"],
      "Interval": "10s",
      "Timeout": "2s"
    }
    ,{
      "Id": "${SERVICE_ID}-tcp",
      "Name": "${SERVICE_ID} tcp",
      "Tcp": "${ETH0_IP}:${SERVICE_PORT}",
      "Interval": "10s",
      "Timeout": "1s"
    }
    // ,{
    //   "Id": "${SERVICE_ID}-docker",
    //   "Name": "${SERVICE_ID} Docker",
    //   "Docker_container_id": "$CONTAINER_ID",
    //   "Shell": "/bin/sh",
    //   "Args": ["/usr/local/scripts/health-entrypoint.sh"],
    //   "Interval": "10s"
    // }
  ],
}
